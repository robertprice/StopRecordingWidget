<?xml version="1.0" encoding="utf-8" ?>
<widget id="StopRecordingWidget.widget.StopRecordingWidget" needsEntityContext="false" xmlns="http://www.mendix.com/widget/1.0/">
    <name>StopRecordingWidget</name>
    <description>Set the title of the page to "Stop recording..."</description>
    <icon></icon>

    <properties>
        <property key="delayMicroseconds" type="integer" required="true" defaultValue="3000">
            <caption>Delay Microseconds</caption>
            <category>Configuration</category>
            <description>The delay in microseconds before the page title is set to "Stop recording..."</description>
        </property>
    </properties>
</widget>
